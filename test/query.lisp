(in-suite query)

(defvar *show-query* nil)

(defmacro with-sql-recording (&body body)
  `(unwind-protect
       (progn
         (start-sql-recording)
         ,@body)
    (stop-sql-recording)))

(defun run-query-tests ()
  (with-sql-recording
         (let ((*show-query* #t))
           (run! 'query))))

(defun debug-query-test (test)
  (with-sql-recording
      (let ((*show-query* #t))
        (debug! test))))

